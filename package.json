{
  "name": "fj16_hankinta_backend",
  "version": "1.0.0",
  "main": "server/server.js",
  "scripts": {
    "start": "node .",
    "test": "npm run linter && npm run test-backend && npm run test-e2e",
    "pretest-e2e": "NODE_ENV=test npm run reset-database",
    "test-e2e": "NODE_ENV=test wdio wdio.conf.js",
