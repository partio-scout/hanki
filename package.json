    "postinstall": "npm run deploy",
    "deploy": "NODE_ENV=production webpack -p --config webpack.production.config.js",
    "test-watch": "NODE_ENV=test mocha --watch -R spec",
    "dev-setup": "./scripts/dev-setup.sh",
    "dev-teardown": "./scripts/dev-teardown.sh",
    "reset-database": "./scripts/reset-db.sh",
    "create-user": "SKIP_DATABASE_UPDATE=true node scripts/create-user.js",
    "dev-login": "SKIP_DATABASE_UPDATE=true node scripts/dev-login.js",
    "console": "loopback-console ."
  },
